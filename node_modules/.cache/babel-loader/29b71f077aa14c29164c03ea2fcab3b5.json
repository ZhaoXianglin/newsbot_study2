ne_id) {\n            //状态加入购物车的状态改为true\n            state.showed_phones[phone_id].add_to_cart = true;\n            state.cart_phones.push(phone_id)\n        },\n        addShowedPhone(state, phone) {\n            //增加一个属性,先默认为true，让按钮不显示\n            phone.add_to_cart = false\n            state.showed_phones[phone.id] = phone\n            state.current_phone = phone\n            state.showed_phones_list.push(phone.id)\n            // 然后修改之前显示的一条\n            // if (state.showed_phones_list.length > 1) {\n            //     let pre_id = state.showed_phones_list[state.showed_phones_list.length - 2]\n            //     if (!state.cart_phones.includes(pre_id)) {\n            //         //如果上一个元素没有被加入购物车\n            //         state.showed_phones[pre_id].add_to_cart = false;\n            //     }\n            // }\n        },\n        setWarning(state, phone_id) {\n            state.showed_phones[phone_id].warning = true;\n        }\n    }\n})\n\nnew Vue({\n    store,\n    router,\n    render: h => h(App),\n}).$mount('#app')\n"],"mappings":";;;;AAAA,OAAOA,GAAP,MAAgB,KAAhB;AACA,OAAOC,GAAP,MAAgB,WAAhB;AACA,OAAOC,IAAP,MAAiB,MAAjB;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,OAAOC,eAAP,MAA4B,mBAA5B;AACA,OAAOC,WAAP,MAAwB,eAAxB;AACA,OAAOC,IAAP,MAAiB,MAAjB;AAEA,OAAO,oBAAP;AACA,OAAO,sBAAP;AAEAN,GAAG,CAACO,MAAJ,CAAWC,aAAX,GAA2B,KAA3B;AACAR,GAAG,CAACS,GAAJ,CAAQP,IAAR;AACAF,GAAG,CAACS,GAAJ,CAAQL,eAAR;AACAJ,GAAG,CAACS,GAAJ,CAAQJ,WAAR;AACAL,GAAG,CAACS,GAAJ,CAAQH,IAAR;AAEA,IAAMI,KAAK,GAAG,IAAIJ,IAAI,CAACK,KAAT,CAAe;EACzBC,KAAK,EAAE;IACH;IACAC,aAAa,EAAE,EAFZ;IAGH;IACAC,aAAa,EAAE,EAJZ;IAKH;IACAC,kBAAkB,EAAE,EANjB;IAOH;IACAC,WAAW,EAAE;EARV,CADkB;EAWzBC,SAAS,EAAE;IACPC,SADO,qBACGN,KADH,EACUO,QADV,EACoB;MACvB;MACAP,KAAK,CAACE,aAAN,CAAoBK,QAApB,EAA8BC,WAA9B,GAA4C,IAA5C;MACAR,KAAK,CAACI,WAAN,CAAkBK,IAAlB,CAAuBF,QAAvB;IACH,CALM;IAMPG,cANO,0BAMQV,KANR,EAMeW,KANf,EAMsB;MACzB;MACAA,KAAK,CAACH,WAAN,GAAoB,KAApB;MACAR,KAAK,CAACE,aAAN,CAAoBS,KAAK,CAACC,EAA1B,IAAgCD,KAAhC;MACAX,KAAK,CAACC,aAAN,GAAsBU,KAAtB;MA